<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogInScene.RequestController">
   <children>
      <Pane fx:id="pane2" prefHeight="700.0" prefWidth="1000.0">
         <children>
            <Button fx:id="logOutButton" layoutX="889.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBackButton" prefHeight="25.0" prefWidth="97.0" text="Go Back" />
            <Pane layoutX="14.0" layoutY="14.0" prefHeight="410.0" prefWidth="601.0">
               <children>
                  <TextArea fx:id="plainText" editable="false" prefHeight="410.0" prefWidth="601.0">
                     <font>
                        <Font name="Courier New" size="12.0" />
                     </font></TextArea>
               </children>
            </Pane>
            <Button fx:id="firstReq" layoutX="769.0" layoutY="14.0" mnemonicParsing="false" onAction="#prevRequestButton" prefHeight="25.0" prefWidth="97.0" text="First Request" />
            <Button fx:id="nextReq" disable="true" layoutX="644.0" layoutY="14.0" mnemonicParsing="false" onAction="#nextRequestButton" prefHeight="25.0" prefWidth="97.0" text="Next Request" />
            <Button fx:id="acceptButton" disable="true" layoutX="714.0" layoutY="61.0" mnemonicParsing="false" onAction="#acceptRequestButton" prefHeight="25.0" prefWidth="97.0" text="Accept" />
            <Line endX="122.0" layoutX="828.0" layoutY="50.0" startX="-123.0" />
            <Button fx:id="declineButton" disable="true" layoutX="828.0" layoutY="61.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="97.0" text="Decline" />
            <Line endX="122.0" layoutX="828.0" layoutY="98.0" startX="-123.0" />
            <Button fx:id="accountInfo" disable="true" layoutX="889.0" layoutY="114.0" mnemonicParsing="false" onAction="#showUserData" prefHeight="25.0" prefWidth="97.0" text="Show User" />
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
