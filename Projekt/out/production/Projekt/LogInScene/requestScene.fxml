<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogInScene.RequestController">
   <columnConstraints>
      <ColumnConstraints/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints/>
   </rowConstraints>
   <Pane  prefHeight="700.0" prefWidth="1000.0">
      <Button  layoutX="889.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBackButton"
              prefHeight="25.0" prefWidth="97.0" text="Vrátiť sa"/>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="410.0" prefWidth="601.0">
         <TextArea fx:id="plainText" editable="false" prefHeight="410.0" prefWidth="601.0">
            <font>
               <Font name="Courier New" size="12.0"/>
            </font>
         </TextArea>
      </Pane>
      <Button  layoutX="763.0" layoutY="14.0" mnemonicParsing="false" onAction="#prevRequestButton"
              prefHeight="25.0" prefWidth="116.0" text="Prvá požiadavka"/>
      <Button fx:id="nextReq" disable="true" layoutX="625.0" layoutY="14.0" mnemonicParsing="false"
              onAction="#nextRequestButton" prefHeight="25.0" prefWidth="116.0" text="Ďalšia požiadavka"/>
      <Button fx:id="acceptButton" disable="true" layoutX="714.0" layoutY="61.0" mnemonicParsing="false"
              onAction="#acceptRequestButton" prefHeight="25.0" prefWidth="97.0" text="Potvrdiť"/>
      <Line endX="122.0" layoutX="828.0" layoutY="50.0" startX="-123.0"/>
      <Button fx:id="declineButton" disable="true" layoutX="828.0" layoutY="61.0" mnemonicParsing="false"
              onAction="#declineRequestButton" prefHeight="25.0" prefWidth="97.0" text="Odmietnuť"/>
      <Line endX="122.0" layoutX="828.0" layoutY="98.0" startX="-123.0"/>
      <Button fx:id="accountInfo" disable="true" layoutX="889.0" layoutY="114.0" mnemonicParsing="false"
              onAction="#showUserData" prefHeight="25.0" prefWidth="97.0" text="Účet žiadateľa"/>
   </Pane>
</GridPane>
