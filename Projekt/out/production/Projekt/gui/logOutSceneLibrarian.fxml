    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <Pane  prefHeight="700.0" prefWidth="1000.0">
        <Button  layoutX="889.0" layoutY="14.0" mnemonicParsing="false"
                onAction="#logOutButtonClicked" prefHeight="25.0" prefWidth="97.0" text="Odhlásiť sa"/>
